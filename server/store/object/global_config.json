{
  "authorizedRedirectUris": [
    "http://localhost:9123/#/workbench/userInfo"
  ],
  "hooksConfiguration": {
    "authentication": {
      "postAuthenticationSwitch": false,
      "mutatingPostAuthenticationSwitch": false
    },
    "restApi": {
      "onRequestSwitch": false,
      "onResponseSwitch": false
    }
  },
  "configureWunderGraphApplication": {
    "security": {
      "allowedHostsEnable": true,
      "enableGraphQLEndpoint": true,
      "allowedHosts": []
    },
    "cors": {
      "allowedOriginsEnable": true,
      "allowedOrigins": [],
      "allowedMethods": [
        "GET",
        "POST"
      ],
      "allowedHeadersEnable": true,
      "allowedHeaders": [
        "*"
      ],
      "exposedHeaders": [
        "*"
      ],
      "exposedHeadersEnable": true,
      "maxAge": 120,
      "allowCredentials": true
    }
  }
}